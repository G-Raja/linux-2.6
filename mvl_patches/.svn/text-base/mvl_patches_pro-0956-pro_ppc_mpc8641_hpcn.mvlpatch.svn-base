+void mpc8641hpcn_restart(char *cmd)
+{
+	bd_t *binfo = (bd_t *) __res;
+	void __iomem *rstcr;
+
+	rstcr = ioremap(binfo->bi_immr_base + MPC86XX_RSTCR_OFFSET, 0x100);
+
+	local_irq_disable();
+
+	/* Assert reset request to Reset Control Register */
+	out_be32(rstcr, 0x2);
+
+	/* not reached */
+}
